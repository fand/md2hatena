{
  "name": "md2hatena",
  "description": "Markdown to Hatena Syntax converter",
  "version": "0.3.0",
  "author": "fand <fand@gmork.in>",
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "bin": "bin/cli.js",
  "bugs": {
    "url": "https://github.com/fand/md2hatena/issues"
  },
  "dependencies": {
    "meow": "^4.0.0",
    "pify": "^3.0.0",
    "remark-parse": "^5.0.0",
    "unified": "^6.1.6"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "dedent": "^0.7.0",
    "eslint": "^4.19.1",
    "eslint-config-xo": "^0.20.1",
    "nyc": "^11.6.0"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "homepage": "https://github.com/fand/md2hatena/issues",
  "keywords": [
    "hatena",
    "markdown",
    "md"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/",
      "bin/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fand/md2hatena"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "test": "nyc ava test",
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint src test",
    "ci": "npm run build && npm run lint && npm run test"
  }
}
